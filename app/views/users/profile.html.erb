<br><br><br><br><br><h2>Edit <%= resource_name.to_s.humanize %></h2>

<%= form_for(resource, as: resource_name, url: profile_user_path(resource_name), html: { method: :put }) do |f| %>

  <div class="field">
    <%= f.label :picture%><br>
    <%= f.file_field :avatar %>
  </div>

  <div class="actions">
    <%= f.submit "Update" %>
  </div>
<% end %>

<%= link_to "Back", :back %>
